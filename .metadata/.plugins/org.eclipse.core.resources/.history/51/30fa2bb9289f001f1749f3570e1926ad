package javaProject;

public class AlphabetPattern {

	public static void main(String[] args) {
		int alphabet=65;
		System.out.println("****Printing the Pattern ****");
		for(int i=0;i<=5;i++) {
			for (int j=0;j<=i;j++) {
				System.out.print((char)(alphabet+j)+"");
				System.out.println("\t");
			}
			System.out.println();
		}
	}

}
