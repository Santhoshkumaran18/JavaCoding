package javaProject;

public class AlphabetPattern {

	public static void main(String[] args) {
		int alphabet=65;
		System.out.println("****Printing the Pattern ****");
		for(int i=0;i<=5;i++) {
			for (int j=0;j<=1;j++) {
				System.out.println((char)(alphabet+j));
			}
			System.out.println();
		}
	}

}
