package InterviewRevision;

import java.util.Scanner;

public class PrimeNumbersUsingFalse {
	
	public static void main(String[] args) {
		Scanner s=new Scanner(System.in);
		isPrimeNumber(10);
	}
	
	public static boolean isPrimeNumber(int num) {
		if(num<1) {
			return false;
		}
		for(int i=2;i<num;i++) {
			if(num %i==0) {
				return false;
			}
		}
		return true;
	}

}
