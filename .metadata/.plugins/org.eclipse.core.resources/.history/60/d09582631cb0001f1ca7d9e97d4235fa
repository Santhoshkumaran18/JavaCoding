public class jdbcConnection {
	public static void main(String[] args) throws SQLException {
		String host="127.0.0.1";
		String port="3306";
		DriverManager.getConnection("jdbc::mysql://" + host + ":" + port + "/databaseName", "root", "Sanjay06@$18");
		
		 
	}

}
