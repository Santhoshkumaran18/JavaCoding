import java.util.*;
public class DuplicateElements {

	public static void main(String[] args) {
		int[] a= {1,1,1,1,2,2,3,4,5,5};
		
		LinkedHashSet<Integer> num=new LinkedHashSet<>();
		for(int array:a) {
			num.add(array);
		}
		
		int[] result = new int[num.size()];
